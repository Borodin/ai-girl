{
  "name": "ai-girlfriend-bot",
  "version": "1.0.0",
  "description": "AI Girlfriend Telegram Bot with multiple characters",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "spell": "cspell \"src/**/*.ts\" --no-progress",
    "check": "npm run typecheck && npm run lint && npm run spell"
  },
  "keywords": [
    "telegram",
    "bot",
    "ai",
    "typescript",
    "sequelize"
  ],
  "author": "Maksim Borodin",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.17.1",
    "axios": "^1.12.1",
    "dotenv": "^16.3.1",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.3.1",
    "openai": "^5.20.1",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "typescript-telegram-bot-api": "^0.10.0",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.8.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "cspell": "^8.15.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.0",
    "prettier": "^3.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
